source "https://rubygems.org"

gem "github-pages", group: :jekyll_plugins

group :jekyll_plugins do
  gem "jekyll-seo-tag"
  gem "jekyll-github-metadata"
  gem "jekyll-avatar"
end

platforms :mingw, :x64_mingw, :mswin, :jruby do
  gem "tzinfo", "~> 1.2"
  gem "tzinfo-data"
end

gem "webrick"
gem 'wdm', '~> 0.1.1', :install_if => Gem.win_platform?

gem "jekyll", "~> 3.9.0"
